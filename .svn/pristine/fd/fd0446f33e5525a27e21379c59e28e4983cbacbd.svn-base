    @ExcelProperty("软件编号")
    @TableField("RJBH_")
    private String rjbh;

    @ExcelProperty("系统名称")
    @TableField("Sys_Name_")
    private String sysName;

    @ExcelProperty("版本号")
    @TableField("BBH_")
    private String bbh;

    @ExcelProperty("来源")
    @TableField("LY_")
    private String ly;

    @ExcelProperty("(准)捐赠单位代码")
    @TableField("JZDWDM_")
    private String jzdwdm;


    @ExcelProperty("(准)捐赠者名称")
    @TableField("JZZMC_")
    private String jzzmc;


    @ExcelProperty("生产国别码")
    @TableField("SCGBM_")
